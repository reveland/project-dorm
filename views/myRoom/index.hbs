{{#if user.hasRoom }}
    room stuff
{{else}}
    <table class="table table-striped table-hover ">
        <thead>
            <tr>
                <th>Szobaszám</th>
                <th>Foglelt-e</th>
                <th>befoglalás</th>
            </tr>
        </thead>
        <tbody>
            {{#each rooms}}
                <tr>
                    <td>{{number}}</td>
                    <td>{{occupied}}</td>
                    <td><a href="/myroom/occupy?{{number}}">Foglal</a></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
{{/if}}
